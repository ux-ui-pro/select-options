{
  "name": "select-options",
  "version": "1.1.1",
  "description": "SelectOptions replaces standard HTML drop-down lists with custom ones, improving their appearance and user interaction.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/select-options.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/select-options/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/select-options",
  "sideEffects": false,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "build": "yarn clean && parcel build",
    "lint:js": "eslint --ext .js",
    "lint:scss": "stylelint **/*.scss",
    "lintfix": "yarn lint:js --fix && yarn lint:scss --fix"
  },
  "css": "dist/index.css",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "targets": {
    "css": {
      "source": "src/index.scss"
    },
    "main": {
      "source": "src/index.ts"
    },
    "module": {
      "source": "src/index.ts"
    },
    "types": {
      "source": "src/index.ts"
    }
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-sass": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "eslint": "^7.32.0 || ^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "parcel": "^2.12.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-selector-bem-pattern": "^4.0.0",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "custom select",
    "field",
    "dropdown",
    "select",
    "auto",
    "options",
    "required",
    "material",
    "design",
    "mobile",
    "desktop",
    "floating",
    "label",
    "notched",
    "outline",
    "form",
    "responsive",
    "ui",
    "ux",
    "javascript",
    "web",
    "development",
    "typescript"
  ]
}
